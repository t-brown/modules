#%Module######################################
# note that this is read by a Tcl interpretor
#
proc ModulesHelp { } {
puts stderr "\tSets up the ImageMagick environment.\n"
}

module-whatis "loads the default ImageMagick environment"

set base        /apps/ImageMagick
set ver         6.9.0
set path        $base/$ver

setenv MAGICK_HOME $path
prepend-path PATH $path/bin
prepend-path MANPATH $path/man
prepend-path LD_LIBRARY_PATH $path/lib

conflict imagemagick
