#%Module######################################################################
##
##      bbftp-client
##
proc ModulesHelp { } {
        puts stderr "Sets up the paths you need to use bbftp-client"
}

set sys         [uname sysname]
set base        /apps/bbftp-client
set ver         3.2.1
set path        $base/$ver

if { $sys == "Linux" } {
   prepend-path PATH $path/bin
}
