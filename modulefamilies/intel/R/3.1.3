#%Module######################################################################
##
##     R
##
proc ModulesHelp { } {
        puts stderr "Sets up the paths you need to use R (compiled with Intel)"
}

set sys         [uname sysname]
set base        /apps/R
set ver         3.1.3-intel
set path        $base/$ver

module-whatis "loads the R package, compiled with Intel"

conflict R

   prepend-path PATH $path/bin
   prepend-path MANPATH $path/man

