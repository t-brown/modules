#%Module######################################
# note that this is read by a Tcl interpretor
# # Copied from Jet by Raghu.
# # Built with help from Chris Harrop and George Carr
#
proc ModulesHelp { } {
global path version
puts stderr "\tncarg - sets up the NCO data manipulation tool."
puts stderr "\tPath $path/*\n"
puts stderr "\tVersion $version\n"
}

module-whatis "loads the NCO data manipulation tool"

conflict nco

set version 4.4.5-intel
set path /apps/nco/$version

prepend-path PATH $path/bin

prepend-path LD_LIBRARY_PATH $path/lib
prepend-path MANPATH $path/share/man

conflict nco
