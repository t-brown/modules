#%Module######################################################################
##
##    netcdf 4 (pgi version)
##
proc ModulesHelp { } {
        puts stderr "Sets up the paths you need to use netcdf 4"
}

set sys         [uname sysname]
set base        /apps/netcdf
set ver         4.3.3-rc2-pgi-mvapich2
set path        $base/$ver

   prepend-path PATH $path/bin
   prepend-path LD_LIBRARY_PATH $path/lib
   prepend-path MANPATH $path/share/man
   setenv NETCDF $base/$ver

conflict netcdf
