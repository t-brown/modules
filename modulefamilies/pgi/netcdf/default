#%Module######################################################################
##
##    netcdf 3
##
proc ModulesHelp { } {
        puts stderr "Sets up the paths you need to use netcdf 3"
}

set sys         [uname sysname]
set base        /apps/netcdf
set ver         3.6.3
set family      pgi
set path        $base/$ver-$family

   prepend-path PATH $path/bin
   prepend-path MANPATH $path/share/man
   setenv NETCDF $base/$ver-$family

conflict netcdf
